{
  "manifest_version": 3,
  "name": "Website Time Blocking",
  "version": "1.0",
  "description": "Control your website usage with blocking types.",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "options.html"
  },
  "options_page": "options.html",
  "icons": {
     "16": "icons/icon16.png",
     "48": "icons/icon48.png",
     "128": "icons/icon128.png"
  }
}
